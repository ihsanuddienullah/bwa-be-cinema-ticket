{
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^express$",
    "^express/(.*)$",
    "^(assert|buffer|child_process|cluster|crypto|dns|events|fs|http|https|net|os|path|querystring|readline|stream|string_decoder|timers|tls|url|util|zlib)$",
    "^[a-zA-Z0-9]",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5"
}